GET     /                           controllers.Application.index
GET     /adduser                    controllers.Application.adduser
GET     /loginuser                  controllers.Application.loginuser
GET     /home                       controllers.Home.welcome
GET     /about                      controllers.About.about

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

POST    /handleForm                 controllers.Application.handleForm
POST    /                           controllers.Application.doLogin
